{
  "name": "app-template",
  "private": true,
  "description": "Basic template for starting a new kiosk or web project",
  "version": "1.0.0",
  "author": "@scimusmn",
  "dependencies": {
    "dotenv": "^8.2.0",
    "gatsby": "^4.2.0",
    "gatsby-plugin-image": "^2.2.0",
    "gatsby-plugin-manifest": "^4.2.0",
    "gatsby-plugin-react-helmet": "^5.2.0",
    "gatsby-plugin-sharp": "^4.2.0",
    "gatsby-source-contentful": "7.0.0",
    "gatsby-source-filesystem": "^4.2.0",
    "gatsby-transformer-sharp": "^4.2.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "swiper": "6.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/register": "^7.12.1",
    "chalk": "^4.1.0",
    "contentful-management": "^7.45.6",
    "depcheck": "^1.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^4.3.0",
    "lodash": "^4.17.20",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "sort-package-json": "^1.44.0",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "clean": "gatsby clean",
    "depcheck": "node -r @babel/register scripts/depcheck.js",
    "develop": "gatsby develop --port 3000",
    "export-space": "node scripts/contentful/export-space.js",
    "populate-space": "node scripts/contentful/populate-space.js",
    "install:arduino-base": "node scripts/contentful/install-arduino-base.js",
    "install:contentful": "node scripts/contentful/install-contentful.js && node scripts/contentful/autogenerate-pages.js",
    "lint": "npm-run-all lint:*",
    "lint:code": "node_modules/.bin/eslint .",
    "lint:style": "node_modules/.bin/stylelint \"**/*.*css\"",
    "serve": "gatsby serve --port 3000"
  }
}
